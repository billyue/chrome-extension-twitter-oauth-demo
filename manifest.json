{
  "name": "test",
  "version": "0.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://api.twitter.com/oauth/authorize"],
      "js": ["content_script.js"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_title": "test",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts":  [
      "jquery.js",
      "sha1.js",
      "oauth.js",
      "const.js",
      "twitter.js",
      "background.js"
    ]
  },
  "permissions": ["https://api.twitter.com/"]
}
